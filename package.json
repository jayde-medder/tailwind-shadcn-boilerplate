{
  "name": "tailwind-shadcn-boilerplate",
  "version": "0.0.1",
  "description": "A React + Vite + Tailwind + ShadCn starter",
  "type": "module",
  "author": "Jade Medder",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch ./src/server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "test": "vitest",
    "knex": "knex --knexfile ./src/server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.0.1",
    "express": "^4.17.2",
    "express-jwt": "^8.4.1",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^3.1.0",
    "knex": "^3.1.0",
    "lucide-react": "^0.364.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.6",
    "superagent": "7.1.1",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^20.12.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/superagent": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.18.19",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "2.6.1",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "superagent": "7.1.1",
    "tailwindcss": "^3.4.3",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^0.34.1"
  }
}
